{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express backend with Prisma ORM",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --create-only",
    "db:migrate:deploy": "prisma migrate deploy",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/s3-request-presigner": "^3.873.0",
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.14",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "locale-codes": "^1.3.1",
    "multer": "^2.0.2",
    "openai": "^5.15.0",
    "subsrt-ts": "^2.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "prisma": "^6.14.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
